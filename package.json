{
  "name": "vue-plugin-vuetify-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve ./dev/main.js",
    "build": "vue-cli-service build --target lib --name vue-plugin-vuetify-base src/plugin.js",
    "build:pack": "vue-cli-service build --target lib --name vue-plugin-vuetify-base src/plugin.js && npm pack",
    "build:demo": "vue-cli-service build --dest demo ./dev/main.js",
    "lint": "vue-cli-service lint --fix && vue-cli-service lint dev --fix"
  },
  "main": "./dist/vue-plugin-vuetify-base.common.js",
  "files": [
    "dist"
  ],
  "dependencies": {},
  "devDependencies": {
    "@mdi/font": "^3.6.95",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "babel-eslint": "^10.0.3",
    "core-js": "^3.6.4",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "roboto-fontface": "*",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.11",
    "vuetify-loader": "^1.4.3"
  }
}
